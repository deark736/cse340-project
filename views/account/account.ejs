<div class="dashboard">
  <h1><%= title %></h1>
  <%- messages() %>

  <h2>Welcome <%= accountData.account_firstname %></h2>

  <% if (accountData.account_type !== 'Client') { %>
    <h3>Inventory Management</h3>
    <p><a href="/inv/" class="btn-link">Go to Inventory Management</a></p>
  <% } %>

  <p><a href="/account/edit/<%= accountData.account_id %>" class="btn-link">Update Account Information</a></p>
</div>